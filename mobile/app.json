{
  "expo": {
    "name": "GroupThat",
    "slug": "groupthat",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "groupthat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dallinhull.groupthat",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "GroupThat needs camera access to let you take a profile picture.",
        "NSPhotoLibraryUsageDescription": "GroupThat needs access to your photos to let you choose a profile picture.",
        "NSContactsUsageDescription": "GroupThat needs access to your contacts to let you invite friends to your groups.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dallinhull.groupthat",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "runtimeVersion": "1.1.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1fdd5250-331a-446a-a605-e00c3f56055f"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/1fdd5250-331a-446a-a605-e00c3f56055f"
    }
  }
}
